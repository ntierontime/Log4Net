@model Log4Net.DataSourceEntities.Log
@{
    ViewBag.Title = Framework.Resources.UIStringResource.DeleteAlternativeText;
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
<h3>@Framework.Resources.UIStringResource.Warning_Areyousureyouwanttodeletethis</h3>
            </hgroup>
        </div>
    </section>
}
<fieldset>
    <legend>@Log4Net.Resources.UIStringResourcePerEntityLog.Log.</legend>
	<div class="display-label">
			@Html.LabelFor(model => model.Id)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Id)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Date)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Date)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Thread)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Thread)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Level)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Level)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Logger)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Logger)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Message)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Message)
        </div>
        <div class="display-label">
			@Html.LabelFor(model => model.Exception)
        </div>
        <div class="display-field">
			@Html.DisplayFor(model => model.Exception)
        </div>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <div class="buttonContainer">
        <input type="submit" value="@Framework.Resources.UIStringResource.DeleteAlternativeText" class="button" />
		<input type="button" value="@Framework.Resources.UIStringResource.BackAlternativeText" class="button" onclick="location.href='@Url.Action("Index")'" />
    </div>
}
</fieldset>